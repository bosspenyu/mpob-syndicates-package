{
    "name": "mpob/syndicates",
    "description": "Syndicates Modules",
    "keywords": [
        "mpob",
        "syndicates"
    ],
    "homepage": "https://github.com/mpob/syndicates",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mu",
            "email": "mu.resources.development@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "spatie/laravel-medialibrary": "^10.15",
        "laravel/ui": "^4.0.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Mpob\\Syndicates\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpob\\Syndicates\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "mpob/syndicates"
            ],
            "providers": [
                "Mpob\\Syndicates\\ServiceProvider"
            ],
            "aliases": {
                "Syndicates": "Mpob\\Syndicates\\Facade"
            }
        }
    }
}
